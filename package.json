{
  "name": "explorer-cli",
  "version": "0.5.1",
  "description": "Command line utility for exploring data from a variety of sources.",
  "license": "MIT",
  "author": "Andrew Bryant <arbryant101@gmail.com> (https://github.com/andybry)",
  "keywords": [
    "redux",
    "cli",
    "terminal",
    "http",
    "postman",
    "data",
    "vim",
    "json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/andybry/explorer-cli.git"
  },
  "engines": {
    "node": ">= 4"
  },
  "bin": {
    "explorer-cli": "./lib/bin.js"
  },
  "main": "./lib/ui/setup",
  "scripts": {
    "build": "rimraf lib && babel app -d lib",
    "prestart": "npm run build",
    "start": "node lib/bin",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "cov": "jest --coverage",
    "cov:watch": "jest --watchAll --coverage",
    "cov:html": "jest --coverage --coverageReporters=html"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "lodash": "4.17.4",
    "redux": "3.7.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-env": "1.5.2",
    "jest": "20.0.4",
    "rimraf": "^2.6.1",
    "strip-ansi": "4.0.0"
  },
  "jest": {
    "clearMocks": true,
    "setupFiles": [
      "./test/setup"
    ],
    "testEnvironment": "node"
  }
}
